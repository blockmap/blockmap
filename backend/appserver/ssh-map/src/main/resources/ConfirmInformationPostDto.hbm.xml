<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.icbc.dataAccess.dto.ConfirmInformationPostDto" table="confirmInformationPost">
		<id name="id" column="comfirm_id">
			<generator class="native"></generator>	
		</id>
		<property name="username"></property>
		<property name="branch"></property>
		<property name="gender"></property>
		<property name="age"></property>
		<property name="email"></property>
		<property name="phone"></property>
		<property name="province"></property>
		<property name="city"></property>
		<property name="district"></property>
		<property name="detailAddress"></property>
		<property name="confirmDate"></property>
		<property name="pathPost"></property>	
		<property name="description"></property>
		<property name="state"></property>
		<property name="user_id"></property>	
	</class>
</hibernate-mapping>