<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<constant name="struts.objectFactory" value="org.apache.struts2.spring.StrutsSpringObjectFactory"/>
    
    <constant name="struts.devMode" value="false"/>
    
    <package name="exception" extends="struts-default">
        <global-results>
            <result name="Exception">404.jsp</result>
        </global-results>
        <global-exception-mappings>
            <exception-mapping exception="java.lang.Exception" result="Exception"/>
        </global-exception-mappings>
    </package>
    
    <package name="user" extends="exception">
      <action name="register" class="RegisterNewUserAction" >
      </action>
      <action name="login" class="LoginUserAction" >
      </action>
      <action name="query" class="QueryUserAction">
      </action>
      <action name="delete" class="DeleteUserAction">
      </action>
      <action name="update" class="UpdateUserAction">
      </action>
      <action name="updatestatus" class="UpdateUserStatusAction">
      	  <result name="success">success.jsp</result>
      	  <result name="fail">404.jsp</result>
      </action>
      <action name="regionalinfection_suspected" class="RegionalInfection_SuspectedAction">
      </action>
      <action name="getuserwithpage" class="GetUserwithPageAction">
      </action>
      <action name="ratio_returnedworker" class="Ratio_ReturnedWorkerAction">
      </action>
      <action name="ratio_fever" class="Ratio_FeverAction">
      </action>
  </package>
	
	<package name="confirm" extends="exception">
		<action name="post" class="AddConfirmAction">
		</action>	
		<action name="updatestate" class="UpdateStateAction">
		</action>
		<action name="getpostwithpage" class="GetPostwithPageAction">
		</action>
	</package>
	
	<package name="report" extends="exception">
		<action name="report" class="AddReportAction">
		</action>	
		<action name="deletereport" class="DeleteReportAction">
		</action>
		<action name="getreportwithpage" class="GetReportwithPageAction">
		</action>
	</package>

	<package name="subinstitution" extends="exception">
		<action name="addsubinstitution" class="AddSubinstitutionAction">
		</action>	
		<action name="deletesubinstitution" class="DeleteSubinstitutionAction">
      </action>
      <action name="updaterisklevel" class="UpdateRiskLevelAction">
      	  <result name="success">success.jsp</result>
      	  <result name="fail">404.jsp</result>
      </action>
      <action name="allsubinstitution" class="AllSubinstitutionAcion">
      </action>
      <action name="findworksubinstitution" class="FindWorkSubinstitutionAction">
      </action>
      <action name="rank5subinstitution" class="Rank5SubinstitutionAction">
      </action>
      <action name="returnableworker" class="ReturnableWorkerAction">
		</action>
		<action name="regionalpedestrianflow" class="RegionalPedestrianflowAction">
		</action>
		<action name="getsubwithpage" class="GetSubwithPageAction">
		</action>
	</package>
	<package name="institution" extends="exception">
		<action name="addinstitution" class="AddInstitutionAction">
			<result name="success">success.jsp</result>
		</action>
		<action name="updateinstitution" class="UpdateInstitutionAction">
      	  <result name="success">success.jsp</result>
      	  <result name="fail">404.jsp</result>
      </action>
		<action name="returnablecompany" class="ReturnableCompanyAction">
		</action>
	</package>





</struts>